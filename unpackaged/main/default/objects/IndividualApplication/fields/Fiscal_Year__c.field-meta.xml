<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fiscal_Year__c</fullName>
    <externalId>false</externalId>
    <formula>IF( YEAR(DATEVALUE(ApprovedDate)) &lt; 2014
,TEXT(YEAR(DATEVALUE(ApprovedDate)))
,IF( DATEVALUE(ApprovedDate) &gt;= DATE(2014,5,1)
,IF( MONTH(DATEVALUE(ApprovedDate)) &lt; 5
,TEXT(YEAR(DATEVALUE(ApprovedDate)))
,TEXT(YEAR(DATEVALUE(ApprovedDate)) + 1)
)
,&quot;2014&quot;
)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Fiscal Year</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
